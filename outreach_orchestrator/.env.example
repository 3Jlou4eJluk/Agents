# Outreach Orchestrator Configuration

# API Keys (Required)
DEEPSEEK_API_KEY=your_deepseek_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Worker Configuration
WORKERS=5                          # Number of parallel workers (3-5 recommended)
CLASSIFICATION_BATCH_SIZE=5        # Batch size for classification
EXECUTOR_MAX_ITERATIONS=50         # Max tool calls per letter generation step

# Model Configuration
CLASSIFICATION_MODEL=deepseek:deepseek-chat
LETTER_GEN_MODEL=deepseek:deepseek-chat

# Optional: LangSmith for tracing
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_PROJECT=outreach-orchestrator
